entity Goblin = {
  inherit UnityGoblin
  
  settings : [(string * int)]
    
  //moving right           
  rule Position, IsMovingLegs =    
    wait Input.GetKey(KeyCode.Space)
    yield Position + new Vector3(dt * 2.0f, 0.0f, 0.0f), true    

  //moving left
  rule Position, IsMovingLegs =
    wait Input.GetKey(KeyCode.C)
    yield Position - new Vector3(dt*2.0f, 0.0f, 0.0f), true
      
  Create(Settings : [(string * int)]) =  
    {
      settings = Settings  
      UnityGoblin = UnityGoblin.Instantiate()
    }
}  